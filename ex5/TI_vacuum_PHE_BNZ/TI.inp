TITLE
Input parameter file for TI simulations
END
STOCHDYN
TO_DO
END
PERTURBATION
TO_DO
END
SYSTEM
#      NPM      NSM
         1        0
END
STEP
#   NSTLIM         T        DT
    500000         0     0.002
END
INITIALISE
# Default values for NTI values: 0
#   NTIVEL    NTISHK    NTINHT    NTINHB
         0         0         0         0
#   NTISHI    NTIRTC    NTICOM
         1         0         0
#   NTISTI
         0
#       IG     TEMPI
    210185         0
END
BOUNDCOND
#      NTB    NDFMIN
         0         0
END
CONSTRAINT
# NTC
    3
#      NTCP  NTCP0(1)
          1    0.0001
#      NTCS   NTCS0(1)
          1    0.0001
END
FORCE
#      NTF array
# bonds    angles   imp.     dihe     charge nonbonded
  0        1        1        1        1      1
# NEGR    NRE(1)    NRE(2)    ...      NRE(NEGR)
     1
      13
END
COMTRANSROT
#     NSCM
      1000
END
PAIRLIST
# algorithm    NSNB   RCUTP   RCUTL    SIZE    TYPE
         0       5     0.8     1.4     0.4       0
END
NONBONDED
# NLRELE
         1
#  APPAK    RCRF   EPSRF
         0       1.4      1     1
# NSHAPE  ASHAPE  NA2CLC   TOLA2   EPSLS
         3       1.4         2     1e-10         0
#    NKX     NKY     NKZ   KCUT
        10        10        10       100
#    NGX     NGY     NGZ  NASORD  NFDORD  NALIAS  NSPORD
        32        32        32         3         2         3         4
# NQEVAL  FACCUR  NRDGRD  NWRGRD
    100000       1.6         0         0
#  NLRLJ  SLVDNS
         0      33.3
END
PRINTOUT
#NTPR: print out energies, etc. every NTPR steps
#NTPP: =1 perform dihedral angle transition monitoring
#     NTPR      NTPP
       500         0
END
WRITETRAJ
# NTPW = 0 : binary
# NTPW = 1 : formatted
# NTWSE = configuration selection parameter
# =0: write normal trajectory
# >0: chose min energy for writing configurations
#    NTWX     NTWSE      NTWV      NTWF      NTWE      NTWG      NTWB
     -500         0         0         0       500       500         0
END
